{
    "app": {
        "name": "FloudsVectors.Py"
    },
    "server": {
        "type": "hypercorn",
        "host": "0.0.0.0",
        "port": 19680,
        "reload": true,
        "workers": 4
    },
    "vectordb": {
        "endpoint": "http://localhost",
        "port": 19530,
        "username": "root",
        "password": "@Milvus2025Milvus#",
        "default_dimension": 256,
        "admin_role_name": "flouds_admin_role",
        "primary_key": "flouds_vector_id",
        "primary_key_data_type": "VARCHAR",
        "vector_field_name": "flouds_vector"
    },
    "logging": {
        "folder": "/var/log/flouds",
        "app_log_file": "flouds-vectordb.log",
        "max_bytes": 10485760,
        "backup_count": 5,
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "date_format": "%Y-%m-%d %H:%M:%S"
    }
}